{
    "version": 2,
    "builds": [{
        "src": "server/index.ts",
        "use": "@vercel/node",
        "config": {
            "maxDuration": 30
        }
    }],
    "routes": [{
            "src": "/api/(.*)",
            "dest": "/server/index.ts"
        },
        {
            "src": "/health",
            "dest": "/server/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}